<UserControl x:Class="SigiFluent.Views.ActionBars.CanceledCheckBookBar"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation"
              >
    <StackPanel Orientation="Horizontal">
        <telerik:RadRibbonGroup HorizontalContentAlignment="Stretch">
            <telerik:RadRibbonButton  LargeImage="/SigiFluent;component/Resources/light/appbar.new.png" Style="{StaticResource RibbonButton}"
										SplitText="False" Size="Large" Text="Nueva Excepción" Command="{Binding NewCommand}"/>
        </telerik:RadRibbonGroup>
        <telerik:RadRibbonGroup Header="Excepcion Seleccionada" >

            <telerik:RadRibbonButton Size="Large" Style="{StaticResource RibbonButton}" SplitText="False" Text="Procesar" 
                    LargeImage="/SigiFluent;component/Resources/light/appbar.lightning.png"
											Command="{Binding DoProcessCommannd}"/>



        </telerik:RadRibbonGroup>

    </StackPanel>
</UserControl>
